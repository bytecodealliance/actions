{
  "name": "github-actions",
  "version": "1.0.0",
  "description": "This provide actions for wasmtime setup",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build -o dist/wasmtime/setup src/wasmtime.ts && ncc build -o dist/wasm-tools/setup src/wasmtools.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytecodealliance/github-actions.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "wasmtime"
  ],
  "author": "Rajat Jindal<rajatjindal83@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1",
    "@octokit/openapi-types": "^19.0.0",
    "@octokit/rest": "^19.0.7",
    "@octokit/types": "^12.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/uuid": "^9.0.1",
    "fs-extra": "^11.1.0",
    "toml": "^3.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.2.5",
    "prettier": "2.8.4",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.5"
  }
}
